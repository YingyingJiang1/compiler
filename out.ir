FUNCTION add :
PARAM v1
t1 := #1 * #4
t2 := v1 + t1
t3 := *v1 + *t2
RETURN t3
FUNCTION main :
DEC v2 8
DEC v3 8
v4 := #0
v5 := #0
LABEL L1 :
IF v4 < #2 GOTO L2
GOTO L6
LABEL L2 :
LABEL L3 :
IF v5 < #2 GOTO L4
GOTO L5
LABEL L4 :
t4 := &v2
t5 := v5 * #4
t6 := t4 + t5
t7 := v4 + v5
*t6 := t7
t8 := v5 + #1
v5 := t8
GOTO L3
LABEL L5 :
t9 := &v3
ARG t4
t10 := CALL add
*t9 := t10
t11 := *t9
WRITE t11
t11 := v4 + #1
v4 := t11
v5 := #0
GOTO L1
LABEL L6 :
RETURN #0
